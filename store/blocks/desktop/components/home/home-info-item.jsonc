  "flex-layout.col#desktop__home--infoitem--one": {
    "children": ["flex-layout.row#home__container--infoitem--one"]
  },
  "flex-layout.row#home__container--infoitem--one": {
    "title": "Item 1: icono y texto",
    "children": [
      "flex-layout.col#home__container--infoitem--one-1",
      "flex-layout.col#home__container--infoitem--one-2"
    ],
    "props": {
      "blockClass": "home__container-infoitem",
      "colSizing": "auto"
    }
  },
  "flex-layout.col#home__container--infoitem--one-1": {
    "children": ["image#home__container--infoitem-one-icon"],
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "middle"
    }
  },
  "image#home__container--infoitem-one-icon": {
    "title": "Item 1: icono",
    "props": {
      "blockClass": "infoitem-icon",
      "src": "assets/icons/icono_pedido.png",
      "link": {
        "url": "/"
      },
      "maxHeight": 24
    }
  },
  "flex-layout.col#home__container--infoitem--one-2": {
    "children": [
      "rich-text#home__container--infoitem--one-text1",
      "rich-text#home__container--infoitem--one-text2"
    ],
    "props": {
      "verticalAlign": "middle",
      "blockClass": "home__container--infoitem"
    }
  },
  "rich-text#home__container--infoitem--one-text1": {
    "title": "Item 1: texto superior",
    "props": {
      "text": "[Sigue tu pedido](/) ",

      "blockClass": "info__item"
