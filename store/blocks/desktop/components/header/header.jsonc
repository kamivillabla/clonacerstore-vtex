{
  "sticky-layout#header__desktop": {
    "children": ["flex-layout.row#header__desktop", "flex-layout.row#mega-menu"],
    "props": {
      "blockClass": "header__desktop"
    }
  },
  "flex-layout.row#header__desktop": {
    "children": [
      "flex-layout.col#header__desktop--logo",
      "flex-layout.col#trigger-mega-menu",
      "flex-layout.col#header__desktop--search-bar",
      "flex-layout.col#header__desktop--other"
    ],
    "props": {
      "colSizing": "auto",
      "colJustify": "around",
      "blockClass": "header__desktop"
        }
  },
  "flex-layout.col#header__desktop--logo": {
    "children": ["logo#header__desktop--logo"],
    "props": { "verticalAlign": "middle" }
  },

  "flex-layout.col#header__desktop--search-bar": {
    "children": ["search-bar#header__desktop--search-bar"],
    "props": { "verticalAlign": "middle" }
  },
  "flex-layout.col#header__desktop--other": {
    "children": ["flex-layout.row#header__other"],
    "props": { "verticalAlign": "middle" }
  },

  "flex-layout.row#header__other": {
    "children": [
      "image#header--phone",
      "rich-text#header--phone-num",
      "login",
      "minicart.v2#header__minicart"
    ],
    "props": {
      "colSizing": "auto"
      // "colJustify": "between"
    }
  }
}
