  "flex-layout.col#home--infoitem-four-two-mobile": {
    "children": ["rich-text#home--infoitem-text-two-one"],
    "props": {
      "blockClass": "infoitem-text-four-one"
    }
  },
  "rich-text#home--infoitem-text-four-one": {
    "title": "Item 3: texto",
    "props": {
      "text": "[contáctanos(/)",
      "blockClass": "info__item-mobil"
    }
  }
}
