  "flex-layout.col#home--infoitem-three-two-mobile": {
    "children": ["rich-text#home--infoitem-text-three-one"],
    "props": {
      "blockClass": "infoitem-text-two-one"
    }
  },
  "rich-text#home--infoitem-text-three-one": {
    "title": "Item 3: texto",
    "props": {
