{
  "flex-layout.row#mobile__home--beneficios": {
    "title": "Sección Beneficios",
    "children": ["flex-layout.col#mobile__home--beneficios"],
    "props": {
      "horizontalAlign": "center",
      "paddingTop": 5
    }
  },
  "flex-layout.col#mobile__home--beneficios": {
    "title": "Contenedor beneficios",
    "children": [
      "flex-layout.row#mobile__home--beneficios-title",
      "flex-layout.row#mobile__home--beneficios-content"
    ]
  },
  "flex-layout.row#mobile__home--beneficios-title": {
    "children": ["rich-text#home--beneficios-title-mobile"],
    "props": {
      "blockClass": "container-home--beneficios-title-mobile"
    }
  },
  "rich-text#home--beneficios-title-mobile": {
    "title": "Título Sección Beneficios",
    "props": {
      "text": "Beneficios de comprar en ACER",
      "blockClass": "home--beneficios-title-mobile"
    }
  },
  "flex-layout.row#mobile__home--beneficios-content": {
    "children": ["flex-layout.col#mobile__home--beneficios-content"]
  },
  "flex-layout.col#mobile__home--beneficios-content": {
    "title": "Contenedor row de beneficios",
    "children": [
      "flex-layout.row#home--beneficios-content-one",
      "flex-layout.row#home--beneficios-content-two",
      "flex-layout.row#home--beneficios-content-three"
    ],
    "props": {
      "preserveLayoutOnMobile": true
    }
  },
  // row 1
  "flex-layout.row#home--beneficios-content-one": {
    "children": [
      "flex-layout.col#beneficios-content-col-one",
      "flex-layout.col#beneficios-content-col-two"
    ],
    "props": {
      "preserveLayoutOnMobile": true
    }
  },
  "flex-layout.col#beneficios-content-col-one": {
    "title": "Beneficios uno",
    "children": [
      "image#home--beneficios--one-img",
      "rich-text#home--beneficios-one-text"
    ],
    "props": {
      "blockClass": ["beneficios-content-col", "beneficios-content-col-line"],
      "horizontalAlign": "center",
      "verticalAlign": "middle"
    }
  },
  "flex-layout.col#beneficios-content-col-two": {
    "title": "beneficios dos",
