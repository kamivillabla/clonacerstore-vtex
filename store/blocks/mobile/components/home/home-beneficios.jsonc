      "image#home--beneficios--three-img",
      "rich-text#home--beneficios-three-text"
    ],
    "props": {
      "blockClass": ["beneficios-content-col", "beneficios-content-col-line"],
      "horizontalAlign": "center",
      "verticalAlign": "middle"
    }
  },
  "flex-layout.col#beneficios-content-col-four": {
    "title": "Beneficios cuatro",
    "children": [
      "image#home--beneficios--four-img",
      "rich-text#home--beneficios-four-text"
    ],
    "props": {
      "blockClass": "beneficios-content-col",
