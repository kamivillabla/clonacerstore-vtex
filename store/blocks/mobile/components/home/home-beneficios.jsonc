  "flex-layout.row#home--beneficios-content-three": {
    "children": [
      "flex-layout.col#beneficios-content-col-five",
      "flex-layout.col#beneficios-content-col-six"
    ],
    "props": {
      "preserveLayoutOnMobile": true
    }
